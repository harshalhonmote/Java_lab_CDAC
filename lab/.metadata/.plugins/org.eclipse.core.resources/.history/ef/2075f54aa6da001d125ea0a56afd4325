package com.harsh.first;

class Demo{
	public int id;
	public String name;
	
	void setTo(int id,String name) {
		this.id =id;
		this.name =name;
	}
	String getTo() {
		return {this.id+" "+this.name};
	}
}

public class Array {
      
	
	public static void main(String[] args) {
		Demo arr[]=new Demo[3];
		

	}

}
