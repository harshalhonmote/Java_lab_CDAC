package com.harsh.first;

import java.util.Scanner;

class Demo{
	public int id;
	public String name;
	
	void setTo(int id,String name) {
		this.id =id;
		this.name =name;
	}
	String getTo() {
		return (this.id+" "+this.name);
	}
}

public class Array {
      
	
	public static void main(String[] args) {
		Demo arr[]=new Demo[3];
		
		for(int i=0;i<arr.length;++i) {
			arr[i]=new Demo();
			System.out.println("Enter ID & NAME:");
			
			
		}

	}

}
