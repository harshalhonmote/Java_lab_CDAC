package com.harsh.first;

class First{
	public int num1;
	public String str;
	public static First f=null;
	
	private First() {
		num1=11;
		str="AAA";
	}
	public static First getRef() {
		if(f == null)f=new First();
		return f;
	}
	public void setTo(int num,String s) {
		this.num1=num;
		this.str=s;
	}
}

public class Practice {

	public static void main(String[] args) {
		First f1= First.getRef();
        System.out.println(f1.num1+" "+f1.str);
        First f2 = First.getRef();
        f2.setTo(33, "KKK");
        System.out.println(f2.num1+" "+f2.str);
          
  
	}

}
