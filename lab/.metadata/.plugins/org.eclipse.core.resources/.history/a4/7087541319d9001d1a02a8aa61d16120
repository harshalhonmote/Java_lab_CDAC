package demo;

public class Pattern {
    
	void pattern01() {
	  for(int i=5;i>=0;--i) {
		  for(int j=i;j>1;j--) {
			  System.out.print("-");
		  }
		  for(int k=i;k<=5;k++) {
			  System.out.print("*");
		  }
		  System.out.println();
	  }
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
      Pattern p = new Pattern();
      p.pattern01();
	}

}
