package geometry;

public class Point2D {
    public int x;
    public int y;
    
    Point2D(){
    	this(0,0);
    }
	public Point2D(int x, int y) {
		this.x = x;
		this.y = y;
	}
	public String getDetails() {
	    return ("POINTS :"+this.x+" "+this.y);	
	}



	public static void main(String[] args) {
		

	}

}
