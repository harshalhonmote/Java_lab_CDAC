package demo;

import java.util.*;

class Second {
	public static void main(String[] ar) {
		System.out.println("hwwwwww");
	}
}


  class First {

	public static void main(String[] args) {
		for(int i=1;i<=8;++i) {
			for(int j=7;j>=1;j--) {
				System.out.print(" ");
			}
			for(int k=1;k<=i;k++) {
				System.out.print("* ");
			}
			System.out.println();
		}
              
	}

}
